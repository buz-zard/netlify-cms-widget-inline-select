{
  "extends": ["@k.sh/eslint-config"],
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": ["!src/*.js"] }],
    "react/jsx-curly-brace-presence": 0,
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
